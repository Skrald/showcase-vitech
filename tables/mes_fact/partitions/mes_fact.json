{
  "name": "mes_fact",
  "mode": "import",
  "source": {
    "type": "m",
    "expression": [
      "let",
      "    src = Table.FromRows(",
      "        Json.Document(",
      "            Binary.Decompress(",
      "                Binary.FromText(",
      "                    \"\",",
      "                    BinaryEncoding.Base64",
      "                ),",
      "                Compression.Deflate",
      "            )",
      "        ),",
      "        let",
      "            _t = ((type nullable text) meta [Serialized.Text = true])",
      "        in",
      "            type table [month_code = _t, manager_id = _t, Sale = _t, Bonus = _t]",
      "    ),",
      "    typed = Table.TransformColumnTypes(",
      "        src,",
      "        {",
      "            {\"month_code\", Int64.Type},",
      "            {\"manager_id\", Int64.Type},",
      "            {\"Sale\", type number},",
      "            {\"Bonus\", type number}",
      "        }",
      "    ),",
      "    added_dt = Table.AddColumn(typed, \"report_dt\", each Date.EndOfMonth(Date.From(Text.From([month_code])&\"01\")), Date.Type)",
      "in",
      "    added_dt"
    ]
  }
}